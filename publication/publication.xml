<?xml version="1.0" encoding="UTF-8" ?>

<publication>

    <!-- HTML-Specific Options -->
    <html>
        <calculator model="geogebra-graphing"/>
        <index-page ref="linear-algebra"/>
        <video privacy="yes"/>
        <!-- baseurl should not have a trailing slash -->
        <baseurl href="https://opentext.uleth.ca/Math3410"/>
    </html>
    <!-- PDF-Specific Options -->
    <pdf>
        <!-- decide whether or not asymptote images in PDF should link to interactive HTML -->
        <!-- requires baseurl to be set -->
        <asy-figs links="yes"/>
    </pdf>
    <latex print="no" sides="one">
      <asymptote links="no"/>
    </latex>
</publication>
